{
  "timeZone": "America/New_York",
  "dependencies": {
    "libraries": [{
      "userSymbol": "OAuth2",
      "libraryId": "1B7FSrk5Zi6L1rSxxTDgDEUsPzlukDsi4KGuTMorsTQHhGBzBkMun4iDF",
      "version": "43"
    }]
  },
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "oauthScopes": [
  <% htmlWebpackPlugin.options.addon.scopes.forEach((scope, index) => { %>
    <% if (index !== htmlWebpackPlugin.options.addon.scopes.length - 1) { %>
      "<%=scope%>",
    <% } else { %>
      "<%=scope%>"
    <% } %>
  <% }) %>
  ],
  "addOns": {
    "common": {
      "name": "<%= htmlWebpackPlugin.options.addon.name %>",
      "logoUrl": "https://pbs.twimg.com/profile_images/1238438046514712577/qD0LQp89_x96.jpg",
      "layoutProperties": {
        "primaryColor": "#ffffff",
        "secondaryColor": "#767676"
      }
    },
    "gmail": {
      "homepageTrigger": {
        "runFunction": "onHomePageOpen",
        "enabled": true
      },
      "contextualTriggers": [{
        "unconditional": {},
        "onTriggerFunction": "onGmailMessageOpen"
      }]
    }
  }
}
